.next {
  width: 40rpx;
  height: 40rpx;
}


.addStock {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99999999999;
}

.addStock .s-b {
  width: 70%;
  margin: 0 auto;
  height: 100rpx;
  background-color: #fff;
  margin-top: 35%;
  border-radius: 10rpx;
}

.addStock .s-b .s-l-b {
  text-align: center;
  width: 80%;
  
  /* border-bottom: 1px solid #f5f5f5; */
  margin: 0 auto;
  padding-top: 10px;
}

.addStock .s-b .s-l-b text {
  font-size: 32rpx;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  color: #333;
  font-weight: bolder;
}

.addStock .s-b button::after {
  border-radius: 4rpx;
  border: 0;
}

.addStock .s-b .l {
  padding: 0px;
  width: 80%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  background-color: #04be01;
  color: #fff;
}

.addStock .s-b .c {
  padding: 0px;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  width: 80%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  background-color: #ccc;
  color: #333;
}