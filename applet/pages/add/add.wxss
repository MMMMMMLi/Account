.searchbar-result {
  margin: 0;
  font-size: 14px;
  position: absolute;
  z-index: 100;
  width: 100%;
}

.searchbar-result .weui-cell__bd {
  padding: 2px 0 2px 20px;
  color: var(--weui-FG-1)
}

.searchbar-result:before {
  display: none
}

/* picker选择框样式 */
.pickerView {
  flex: 0.5;
  padding: 0;
  display: flex;
  padding-left: 16px;
  position: relative;
  align-items: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
}

.mask {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #999;
  z-index: 9999;
  top: 0;
  left: 0;
}