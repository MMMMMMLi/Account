.no-order {
  width: 100%;
  position: absolute;
  bottom: 0;
  top: 88rpx;
  left: 0;
  right: 0;
  text-align: center;
  padding-top: 203rpx;
  background-color: #F2f2f2;
}

.no-order-img {
  width: 81rpx;
  height: 96rpx;
  margin-bottom: 31rpx;
}

.no-order .text {
  font-size: 28rpx;
  color: #999999;
  text-align: center
}

.order-list {
  width: 100%;
  /* height: 300px; */
  margin-bottom: 10px;
}

.order-list .a-order {
  width: 100%;
  background-color: #fff;
  /* background-color: #33CC00; */
  margin-top: 20rpx;
}

.tab-content {
  height: 100%px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}

.tab-time {
  width:50%;    
  display: inline-block;
  padding-left: 20px;
}
.tab-status{
  width:50%;    
  display: inline-block;
  text-align: right;
  padding-right: 20px;
}

.divLine {
  background: #E0E3DA;
  width: 100%;
  height: 5rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.orderList {
  display: flex;
}

.goods-img {
  width: 150rpx;
  height: 150rpx;
  padding: 10px 20px 10px 10px;
  border: 1px solid #000000;
  border-top: none;
  border-bottom: none;
  border-left: none;
}

.order-title {
  margin-left: 30rpx;
  font-weight: bold;
  font-size: 20px;
}

.order-test {
  margin-left: 30rpx;
}

.goods-price {
  font-size: 26rpx;
  width: 720rpx;
  text-align: right;
  margin-bottom: 20rpx;
}

.productInfo {
  /* width: 33%; */
  margin-left: 20rpx;
}

.goods-price .p {
  font-size: 36rpx;
  color: #e64340;
  margin-bottom: 10px;
}

/*scroll-view外层*/
.skill-sequence-panel-content-wrapper {
  position: relative;
  height: 55px;
  white-space: nowrap;
  padding: 2px 10px;
}

/*scroll-view本身*/
.skill-sequence-panel-content {
  min-width: 100%;
}

/*scroll-view内层*/
.skill-sequence-skill-wrapper {
  /* margin-left: 2px; */
  display: inline-block;
}